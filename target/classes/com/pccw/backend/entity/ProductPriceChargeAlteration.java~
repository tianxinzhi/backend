package com.pccw.backend.entity;

import java.io.Serializable;
import javax.persistence.*;
import java.math.BigDecimal;


/**
 * The persistent class for the TM_PRODUCT_PRICE_CHARGE_ALTERATION database table.
 * 
 */
@Entity
@Table(name="TM_PRODUCT_PRICE_CHARGE_ALTERATION")
@NamedQuery(name="ProductPriceChargeAlteration.findAll", query="SELECT p FROM ProductPriceChargeAlteration p")
public class ProductPriceChargeAlteration implements Serializable {
	private static final long serialVersionUID = 1L;

	@Column(name="PRODUCT_PRICE_ALTERATION_ID")
	private BigDecimal productPriceAlterationId;

	@Column(name="PRODUCT_PRICE_CHARGE_ID")
	private BigDecimal productPriceChargeId;

	public ProductPriceChargeAlteration() {
	}

	public BigDecimal getProductPriceAlterationId() {
		return this.productPriceAlterationId;
	}

	public void setProductPriceAlterationId(BigDecimal productPriceAlterationId) {
		this.productPriceAlterationId = productPriceAlterationId;
	}

	public BigDecimal getProductPriceChargeId() {
		return this.productPriceChargeId;
	}

	public void setProductPriceChargeId(BigDecimal productPriceChargeId) {
		this.productPriceChargeId = productPriceChargeId;
	}

}
